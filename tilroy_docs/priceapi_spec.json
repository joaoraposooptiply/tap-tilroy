  "servers": [
    {
      "url": "https://api.tilroy.com/priceapi/production"
    }
  ],
  "components": {
    "parameters": {
      "tilroyApiKey": {
        "name": "Tilroy-Api-Key",
        "in": "header",
        "required": true,
        "description": "API key to identify and validate the tenant",
        "schema": {
          "type": "string"
        }
      },
      "xapikey": {
        "name": "x-api-key",
        "in": "header",
        "required": true,
        "description": "API key of AWS",
        "schema": {
          "type": "string"
        }
      },
      "pathShopId": {
        "name": "shopId",
        "in": "path",
        "required": true,
        "description": "Identifier of the shop to get the price for.",
        "schema": {
          "type": "string"
        }
      },
      "pathSkuId": {
        "name": "skuId",
        "in": "path",
        "required": true,
        "description": "Identifier of the sku to get the price for.",
        "schema": {
          "type": "string"
        }
      },
      "date": {
        "name": "date",
        "in": "query",
        "required": false,
        "description": "Provide this when you want the price on a specific date(ISO 8601 format).",
        "schema": {
          "type": "string",
          "format": "ISO 8601"
        }
      },
      "fields": {
        "name": "fields",
        "in": "query",
        "required": false,
        "description": "Comma-separated list of all fields you want returned in the result. All other fields will be omitted.",
        "schema": {
          "type": "string"
        }
      },
      "filterCountryCode": {
        "name": "countryCode",
        "in": "query",
        "required": false,
        "description": "Filter the result on all prices for this sku and this country. Should be a standard 2-letter ISO code. (Shop country will be used if not available)",
        "schema": {
          "type": "string"
        }
      },
      "filterShopId": {
        "name": "shopId",
        "in": "query",
        "required": false,
        "description": "Filter the result on all prices for this sku and this shop. (shopId or shopNumber required)",
        "schema": {
          "type": "string"
        }
      },
      "filterShopIdOptional": {
        "name": "shopId",
        "in": "query",
        "required": false,
        "description": "Optional filter to get price rules for a specific shop",
        "schema": {
          "type": "string"
        }
      },
      "filterShopNumber": {
        "name": "shopNumber",
        "in": "query",
        "required": false,
        "description": "Filter the result on all prices for this sku and this shop. (shopId or shopNumber required)",
        "schema": {
          "type": "string"
        }
      },
      "filterSkuIds": {
        "name": "skuIds",
        "in": "query",
        "required": true,
        "description": "Comma-separated list of all skuIdentifiers you want a price for. This is limited to a maximum of 100 ids.",
        "schema": {
          "type": "string"
        }
      },
      "filterSkuSourceIds": {
        "name": "skuSourceIds",
        "in": "query",
        "required": true,
        "description": "Comma-separated list of all sku sourceIds you want a price for. This this takes priority over skuIds passed.",
        "schema": {
          "type": "string"
        }
      },
      "priceRange": {
        "name": "priceRange",
        "in": "query",
        "required": false,
        "description": "Determine what price range to use for this call (\"retail\", \"wholesale\" or a custom range).",
        "schema": {
          "type": "string",
          "default": "retail"
        }
      },
      "dateQuery": {
        "name": "date",
        "in": "query",
        "required": false,
        "description": "Datetime (ISO 8601) to get prices on a specified point in time (for future or historic prices, you don't need to use this to get the current price) - 2020-01-01T00:00:00Z",
        "schema": {
          "type": "string",
          "format": "date-time"
        }
      },
      "countParam": {
        "name": "count",
        "in": "query",
        "required": false,
        "description": "Number of results to return for pagination",
        "schema": {
          "type": "integer"
        }
      },
      "pageParam": {
        "name": "page",
        "in": "query",
        "required": false,
        "description": "Page number for pagination",
        "schema": {
          "type": "integer"
        }
      },
      "lastIdParam": {
        "name": "lastId",
        "in": "query",
        "required": false,
        "description": "Cursor for pagination - the ID of the last item from the previous page. Cannot be used together with the page parameter.",
        "schema": {
          "type": "string"
        }
      },
      "dateModifiedParam": {
        "name": "dateModified",
        "in": "query",
        "required": false,
        "description": "Filter by modification date",
        "schema": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "requestBodies": {
      "PricesPayload": {
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/PricesPayload"
            }
          }
        },
        "description": "Payload to import",
        "required": true
      }
    },
    "schemas": {
      "Price": {
        "type": "object",
        "properties": {
          "tilroyId": {
            "type": "string",
            "default": null
          },
          "sourceId": {
            "type": "string",
            "default": null
          },
          "price": {
            "type": "number"
          },
          "type": {
            "type": "string",
            "enum": [
              "promo",
              "standard"
            ]
          },
          "quantity": {
            "type": "number",
            "default": 1
          },
          "run": {
