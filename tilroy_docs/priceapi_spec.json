    "/prices": {
      "get": {
        "summary": "Get current price for multiple skus",
        "description": "This endpoint acts similar to GET /prices/{skuId}/shop/{shopId}, but is meant to be used when you want the current price for multiple skuIds. Either skuIds or skuSourceIds must be passed",
