          },
          {
            "$ref": "#/components/parameters/filterSkuSourceIds"
          },
          {
            "$ref": "#/components/parameters/filterShopId"
          },
          {
            "$ref": "#/components/parameters/filterShopNumber"
          },
          {
            "$ref": "#/components/parameters/filterCountryCode"
          },
          {
            "$ref": "#/components/parameters/priceRange"
          },
          {
            "$ref": "#/components/parameters/dateQuery"
          },
          {
            "$ref": "#/components/parameters/fields"
          }
        ],
        "responses": {
          "200": {
            "description": "ok",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CurrentPrice"
                  }
                }
              }
            }
          },
          "400": {
            "description": "bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
